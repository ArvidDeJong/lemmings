{
  "name": "darvis/lemmings",
  "description": "A fun Lemmings easter egg for Laravel applications to prove developer ownership",
  "homepage": "https://darvis.nl",
  "keywords": [
    "darvis",
    "laravel",
    "easter-egg",
    "lemmings"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Arvid de Jong",
      "email": "info@arvid.nl",
      "homepage": "https://darvis.nl"
    }
  ],
  "autoload": {
    "psr-4": {
      "Darvis\\Lemmings\\": "src/"
    }
  },
  "require": {
    "php": "^8.2",
    "laravel/framework": "^11.0|^12.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Darvis\\Lemmings\\Laravel\\Providers\\DarvisLemmingsProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}